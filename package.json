{
  "name": "logseq-solarized-extended-theme",
  "description": "Solarized Logseq theme with extra stuff",
  "version": "1.17.0",
  "author": "yoyurec <yoyurec@gmail.com>",
  "main": "dist/index.html",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && cp -f src/*.css dist/assets",
    "preinstall": "npx only-allow pnpm",
    "semantic-release": "semantic-release"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@logseq/libs": "^0.0.6"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "conventional-changelog-conventionalcommits": "^5.0.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.20.0",
    "typescript": "^4.7.4",
    "vite": "2.9.14",
    "vite-plugin-logseq": "^1.1.2",
    "semantic-release": "^19.0.3"
  },
  "license": "MIT",
  "logseq": {
    "title": "Solarized Extended Theme",
    "icon": "./icon.png",
    "themes": [
      {
        "name": "Solarized Extended Theme",
        "url": "dist/theme.css",
        "description": "Light sepia-solarized Logseq theme with extra stuff"
      }
    ],
    "id": "logseq-solarized-extended-theme"
  },
  "repository": "https://github.com/yoyurec/logseq-solarized-extended-theme.git"
